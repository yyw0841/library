struct ListNode *getIntersectionNode(struct ListNode *headA, struct ListNode *headB) 
{
    struct ListNode *posA = headA;
    struct ListNode *posB = headB;

    if (!headA || !headB)
        return NULL;

    while (posA != posB) {
        posA = posA ? posA->next : headB;
        posB = posB ? posB->next : headA;
    }
    return posA;
}
